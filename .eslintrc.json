{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-multi-spaces": "error",
        "semi": ["warn", "always"],
        "quotes": ["warn", "double"]
    }
}
